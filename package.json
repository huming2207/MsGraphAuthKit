{
  "name": "msgraph_authkit",
  "version": "0.1.0",
  "description": "The missing NodeJS (server-side/embedded devices) authentication provider library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "debug": "node --inspect=5858 -r ts-node/register src/index.ts",
    "convert": "ts-jest config:migrate ./jest.config.js",
    "test": "jest ./src --watch",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint:list": "eslint */**/*.js --fix",
    "lint": "eslint */**/*.js --fix",
    "start:prod": "nodemon src/index.ts"
  },
  "keywords": [
    "Microsoft",
    "Azure",
    "ms",
    "Microsoft Graph",
    "msg",
    "msgraph"
  ],
  "author": "Jackson Ming Hu <huming2207@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/huming2207/MsGraphAuthKit"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@microsoft/microsoft-graph-client": "^2.0.0",
    "@types/jest": "^25.0.0",
    "@types/node": "^12.7.11",
    "axios": "^0.19.0",
    "moment": "^2.24.0",
    "query-string": "^6.8.3",
    "ts-jest": "^26.0.0",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@babel/polyfill": "7.8.7",
    "@microsoft/microsoft-graph-types": "1.12.0",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "26.0.1",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "ts-node": "8.10.1"
  },
  "files": [
    "dist/**/*"
  ]
}
